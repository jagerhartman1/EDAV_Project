library(ggplot2)
library(gridExtra)
library(dplyr)
edav_table <- read.csv("~/EDAV/edav_table.csv")
interrogative = c("which", "whose", "what", "would", "who", "when", "where", "can", "whom", "how", "why", "whether")
texts = c('RN', 'RY', 'DN', 'DY')
totals = c(11, 356, 291, 44)
table = edav_table[order(-edav_table$total),]
top_50 = table[20:50,]
p1 <- ggplot(data=top_50, aes(x=X1, y=RN)) + geom_bar(stat="identity")  + theme(axis.text.x=element_text(angle=90,hjust=1,vjust=0.5)) + labs(title='Republicans Voted No', x='Word', y='Frequency')
p2 <- ggplot(data=top_50, aes(x=X1, y=RY)) + geom_bar(stat="identity") + theme(axis.text.x=element_text(angle=90,hjust=1,vjust=0.5)) + labs(title='Republicans Voted Yes', x='Word', y='Frequency')
p3 <- ggplot(data=top_50, aes(x=X1, y=DN)) + geom_bar(stat="identity") + theme(axis.text.x=element_text(angle=90,hjust=1,vjust=0.5)) + labs(title='Democrats Voted No', x='Word', y='Frequency')
p4 <- ggplot(data=top_50, aes(x=X1, y=DY)) + geom_bar(stat="identity") + theme(axis.text.x=element_text(angle=90,hjust=1,vjust=0.5)) + labs(title='Democrats Voted Yes', x='Word', y='Frequency')
grid.arrange(p1,p2,p3,p4)
